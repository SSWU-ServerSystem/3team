<head>
    <title>공지사항 확인</title>
    <style>
	table {
	    margin-left: 30px;
    	    width: 90%;
    	    border-collapse: collapse;
	}

	th, td {
    	    padding: 10px;
    	    border: 1px solid #ddd;
    	    text-align: left;
	}

	th {
    	    background-color: #f2f2f2;
	}
    </style>
</head>
<body>
    <div class="main-content">
        <h2>공지사항 확인</h2>
	<table>
            <thead>
                <tr>
                    <th>공지번호</th>
                    <th>제목</th>
                    <th>작성일</th>
                    <th>내용</th>
                    <th>지점명</th>
                </tr>
            </thead>
            <tbody>
                <% notices.forEach(notice => { %>
                    <tr>
                        <td><%= notice.noticeNumber %></td>
                        <td><%= notice.title %></td>
                        <td><%= notice.created_at %></td>
                        <td><%= notice.contents %></td>
                        <td><%= notice.subscriberName %></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </div>
</body>
